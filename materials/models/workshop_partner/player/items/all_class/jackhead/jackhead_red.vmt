"VertexlitGeneric"
{
	"$baseTexture" "models/workshop_partner/player/items/all_class/jackhead/jackhead_red"
	
	">=DX90"
	{
		"$selfillum" 50
	}

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" "6"

	"$yellow" "0"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "2"

	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}

		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}

		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}

		"YellowLevel"
		{
			"resultVar" "$yellow"
		}

		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}

